c4p:oceanography rdf:type skos:Concept ; 
	skos:prefLabel   "oceanography"  ; 
	skos:narrower   c4p:bathymetry  ; 
	skos:narrower   c4p:deepSeaResearch  ; 
	skos:narrower   c4p:marineGeoscience  ; 
	skos:narrower   c4p:paleobathymetry  ; 
