	skos:prefLabel   "physicalGeology"  ; 
	skos:narrower   c4p:drilling  ; 
	skos:narrower   c4p:economicGeology  ; 
	skos:narrower   c4p:energyResources  ; 
	skos:narrower   c4p:geodesy  ; 
